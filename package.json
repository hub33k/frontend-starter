{
  "name": "@hub33k/frontend-starter",
  "version": "0.0.2",
  "description": "Best frontend starter kit in the world",
  "author": "hub33k <hub33k@internet.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "frontend/config/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna clean",
    "rebuild": "yarn run clean && rm -rf node_modules && yarn run bootstrap",
    "dev": "lerna exec --scope @hub33k/frontend-starter-webpack -- yarn run dev",
    "server": "lerna exec --scope @hub33k/frontend-starter-webpack -- yarn run server",
    "debug": "lerna exec --scope @hub33k/frontend-starter-debug -- yarn run start"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "gulp": "^4.0.0",
    "lerna": "^3.4.1"
  },
  "dependencies": {
  }
}

{
  "name": "frontend-starter",
  "version": "0.0.2",
  "description": "Best frontend starter kit in the world",
  "author": "hub33k <hub33k@internet.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preprod": "gulp clean:dist",
    "prod": "NODE_ENV=production webpack --config ./frontend/config/webpack/webpack.prod.babel.js -p --progress --colors",
    "predev": "gulp clean:tmp",
    "dev": "NODE_ENV=development webpack --config ./frontend/config/webpack/webpack.dev.babel.js",
    "gulp": "NODE_ENV=development gulp",
    "server": "NODE_ENV=development babel-node ./frontend/config/webpack/webpack.dev-server.babel.js",
    "prewatch": "gulp clean:tmp",
    "watch": "NODE_ENV=development webpack --watch --config ./frontend/config/webpack/webpack.dev.babel.js"
  },
  "dependencies": {
    "normalize.css": "^8.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^1.0.0",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "gulp": "^4.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.8.3",
    "nunjucks": "^3.1.2",
    "nunjucks-isomorphic-loader": "^2.0.2",
    "postcss-cli": "^6.0.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-bundle-tracker": "^0.3.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.1"
  }
}

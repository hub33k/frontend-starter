{
  "name": "frontend-starter",
  "version": "0.0.1",
  "description": "Best frontend starter kit in the world",
  "author": "hub33k <hub33k@internet.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "gulp clean:dist",
    "build": "NODE_ENV=production webpack --config ./frontend/config/webpack/webpack.prod.babel.js -p --progress --colors",
    "predev": "gulp clean:tmp",
    "dev": "NODE_ENV=development webpack --config ./frontend/config/webpack/webpack.dev.babel.js",
    "gulp": "NODE_ENV=development gulp",
    "server": "NODE_ENV=development babel-node ./frontend/config/webpack/webpack.dev-server.babel.js",
    "test": "eslint ./frontend/src/scripts --config eslintrc.config.js && flow && jest --coverage --config jest.config.js",
    "prewatch": "gulp clean:tmp",
    "watch": "NODE_ENV=development webpack --watch --config ./frontend/config/webpack/webpack.dev.babel.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "jquery": "^3.2.1",
    "normalize.css": "^8.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "copy-webpack-plugin": "^4.3.1",
    "css-loader": "^0.28.8",
    "del": "^3.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "gulp": "^3.9.1",
    "html-loader": "^0.5.4",
    "html-webpack-plugin": "^3.1.0",
    "node-sass": "^4.7.2",
    "nunjucks": "^3.0.1",
    "nunjucks-isomorphic-loader": "^2.0.2",
    "postcss-cli": "^4.1.1",
    "postcss-loader": "^2.0.10",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-tracker": "^0.2.1",
    "webpack-dev-server": "^2.10.1"
  }
}

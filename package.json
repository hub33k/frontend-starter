{
  "private": true,
  "name": "frontend-starter",
  "version": "1.0.0",
  "description": "Best frontend starter kit in the world",
  "author": "hub33k <hub33k@internet.com>",
  "license": "MIT",
  "workspaces": [
    "frontend/config/*"
  ],
  "scripts": {
    "lerna:compile": "lerna run --parallel compile",
    "lerna:compile:watch": "lerna run --parallel compile:watch",
    "lerna:bootstrap": "lerna bootstrap",
    "lerna:clean": "lerna clean",
    "reinstall": "yarn run lerna:clean && rm -rf node_modules && yarn run lerna:bootstrap",
    "webpack:dev": "lerna exec --scope @hub33k/frontend-starter-webpack -- yarn run dev",
    "webpack:server": "lerna exec --scope @hub33k/frontend-starter-webpack -- yarn run server",
    "webpack:start": "lerna exec --scope @hub33k/frontend-starter-webpack -- yarn run start",
    "dev": "yarn run webpack:dev",
    "server": "yarn run webpack:server"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "gulp": "^4.0.0",
    "lerna": "^3.4.1"
  },
  "dependencies": {}
}
